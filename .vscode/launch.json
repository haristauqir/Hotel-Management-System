  "version": "0.2.0",
  "configurations": [
  {
    "name": "WEB MVC",
    "type": "coreclr",
    "request": "launch",
    "preLaunchTask": "build",
    "program": "${workspaceFolder}/Web/WebMVC/bin/Debug/netcoreapp3.1/WebMVC.dll",
    "args": [],
    "cwd": "${workspaceFolder}/Web/WebMVC/",
    "stopAtEntry": false,
    "serverReadyAction": {
      "action": "openExternally",
      "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
    },
    "env": {
      "ASPNETCORE_ENVIRONMENT": "Development"
    },
    "sourceFileMap": {
      "/Views": "${workspaceFolder}/Views"
    }
  },
